@page
@model AboutModel
@{
    ViewData["Title"] = "Về S.T.A.M.P";
}
<head>
    <link rel="stylesheet" href="~/css/about.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/about-responsive.css" asp-append-version="true"/>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
</head>
<body>
    <script src="~/js/navbar.js"></script>
    <script src="~/js/common.js"></script>
    <script src="~/js/timing.js"></script>
    <div class="light-light-blue-background container-fluid" style="padding-bottom: 40px">
        <div class="row app__content">
            <div class="col-2">
                <div class="stamp-choose">
                    <ul class="stamp-list">
                        <li class="stamp-item stamp-item--active">
                            <a href="#" class="stamp-item__link blue-color" id="stamp-2023">S.T.A.M.P 2023</a>
                            <ul class="select-part">
                                <li class="select-item">
                                    <a href="#topic" class="select-part__link">Chủ đề</a>
                                </li>
                                <li class="select-item">
                                    <a href="#objective" class="select-part__link">Đối tượng</a>
                                </li>
                                <li class="select-item">
                                    <a href="#rule" class="select-part__link">Thể lệ</a>
                                </li>
                            </ul>
                        </li>
                        <li class="stamp-item">
                            <a href="#" class="stamp-item__link" id="stamp-2022">S.T.A.M.P 2022</a>
                        </li>
                        <li class="stamp-item">
                            <a href="#" class="stamp-item__link" id="stamp-2021">S.T.A.M.P 2021</a>
                        </li>
                    </ul>
                    <script>
                        $("#stamp-2023").on("click", function() {
                            $("#content-2023").removeClass("display-none");
                            $("#content-2022").addClass("display-none");
                            $("#content-2021").addClass("display-none");
                            $("#stamp-2023").addClass("blue-text");
                            $("#stamp-2022").removeClass("blue-text");
                            $("#stamp-2021").removeClass("blue-text");
                        });
                        $("#stamp-2022").on("click", function() {
                            $("#content-2023").addClass("display-none");
                            $("#content-2022").removeClass("display-none");
                            $("#content-2021").addClass("display-none");
                            $("#stamp-2023").removeClass("blue-color");
                            $("#stamp-2022").addClass("blue-color");
                            $("#stamp-2021").removeClass("blue-color");
                            resize_frame();
                        });
                        $("#stamp-2021").on("click", function() {
                            $("#content-2023").addClass("display-none");
                            $("#content-2022").addClass("display-none");
                            $("#content-2021").removeClass("display-none");
                            $("#stamp-2023").removeClass("blue-color");
                            $("#stamp-2022").removeClass("blue-color");
                            $("#stamp-2021").addClass("blue-color");
                            resize_frame();
                        });
